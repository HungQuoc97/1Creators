<?xml version="1.0" encoding="UTF-8"?>	
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="TestSuite" parallel="tests" thread-count="3">
    <parameter name="url" value="https://1creator.stg.onidservice.cloud/vi/auth/login" />

    <listeners>
        <listener class-name="reportConfig.ExtentTestListener" />
        <listener class-name="reportConfig.ReportNGListenner" />
    </listeners>

    <test name="Run on Chrome">
        <parameter name="browser" value="Chrome" />
        <parameter name="os" value="MAC_OXS" />
        <parameter name="platform" value="IOS" />
        <classes>
          <!-- <class name="com.chub.admin.Login_Account" /> -->
            <class name="com.chub.admin.Search_Article_List" />
        </classes>
            
    </test>
</suite>


